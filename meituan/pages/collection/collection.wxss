.container{
    width: 100%;
    height: 100vh;
    padding: 0;
    margin: 0;
    background-color: #f5f5f5;
}
.nav_head{
    position: relative;
    background-color: #fff;
    display: flex;
    width: 100%;
    height: 80rpx;
}
.business{
    line-height: 80rpx;
    flex: 1;
    text-align: center;
}
.goods{
    line-height: 80rpx;
    flex: 1;
    text-align: center;
}
.bottomLine{
    position: absolute;
    background-color: #f6c543;
    width: 49.5rpx;
    height: 6rpx;
    bottom: 0;
    border-radius: 6rpx;
}
.collection_box{
    background-color: #fff;
}
.collection_info{
    display: flex;
    width: 100%;
    height: 217.2rpx;
    padding: 0 20.5rpx;
    box-sizing: border-box;
}
.business_pic{
    margin: 30rpx 0;
}
.business_pic image{
    width: 159rpx;
    height: 159rpx;
}
.desc{
    padding: 30rpx 0 0 20.5rpx;
}
.brand{
    overflow: hidden;
    width: 428rpx;
    height: 32rpx;
    line-height: 32rpx;
    font-size: 32rpx;
    text-overflow:ellipsis;
    white-space: nowrap;
}
.rank{
    display: flex;
}
.type{
    font-size: 26rpx;
    color: #6b6b6b;
}
.dist{
    padding: 30rpx 20.5rpx 0 0;
    color: #6b6b6b;
    font-size: 26rpx;
}
